<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DirectorAPP.Views.PrincipalView"
                  xmlns:local="clr-namespace:DirectorAPP.Views"
             Title="Main" BarBackgroundColor="{StaticResource ColorAzul}" 
             BarTextColor="{StaticResource ColorBlanco}"
            
             xmlns:vm="clr-namespace:DirectorAPP.ViewModel" NavigationPage.HasNavigationBar="False">
    
    <TabbedPage.BindingContext>
        <vm:DirectorViewModel/>
    </TabbedPage.BindingContext>
    <local:UsuariosView Title="Usuarios" IconImageSource="usuarios.png"/>
    <local:DocentesView Title="Docentes" IconImageSource="profesor.png"/>
   
</TabbedPage>